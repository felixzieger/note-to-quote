{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "node_modules/canvas/**/*"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        }
    ],
    "installCommand": "npm install --build-from-source=canvas"
}